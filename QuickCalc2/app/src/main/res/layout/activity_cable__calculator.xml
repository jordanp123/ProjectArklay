<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="arklay.quickcalc.Cable_Calculator">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Shielded:"
        android:id="@+id/textView8"
        android:layout_marginTop="42dp"
        android:layout_below="@+id/button4"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Insulation(KV):"
        android:id="@+id/textView9"
        android:layout_below="@+id/radioGroup3"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Beam me up"
        android:id="@+id/button4"
        android:onClick="Calculate"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/radioGroup2"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView4">


        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Power Feeder"
            android:id="@+id/radioButton"
            android:checked="true"
            android:layout_above="@+id/textView8"
            android:layout_toEndOf="@+id/radioGroup2" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Portable"
            android:id="@+id/radioButton8"
            android:checked="false"
            android:layout_below="@+id/radioGroup2"
            android:layout_alignParentStart="true" />
    </RadioGroup>

    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/radioGroup"
        android:layout_below="@+id/textView9"
        android:layout_alignParentStart="true">

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2KV"
            android:id="@+id/radioButton3"
            android:checked="true"
            android:layout_alignTop="@+id/textView9"
            android:layout_alignEnd="@+id/radioButton2" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5KV"
            android:id="@+id/radioButton4"
            android:checked="false"
            android:layout_alignTop="@+id/radioButton3"
            android:layout_toEndOf="@+id/radioButton" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="8KV"
            android:id="@+id/radioButton5"
            android:checked="false"
            android:layout_below="@+id/radioButton3"
            android:layout_alignEnd="@+id/textView8" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="15KV"
            android:id="@+id/radioButton6"
            android:checked="false"
            android:layout_below="@+id/radioButton3"
            android:layout_alignStart="@+id/radioButton3" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="25KV"
            android:id="@+id/radioButton7"
            android:checked="false"
            android:layout_below="@+id/radioButton4"
            android:layout_alignStart="@+id/radioButton4" />


    </RadioGroup>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cable Type:"
        android:id="@+id/textView4"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:id="@+id/radioGroup3">

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="UnShielded"
            android:id="@+id/radioButton9"
            android:layout_below="@+id/radioButton2"
            android:layout_alignParentStart="true"
            android:checked="true" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Shielded"
            android:id="@+id/radioButton2"
            android:checked="false"
            android:layout_below="@+id/radioGroup3"
            android:layout_alignParentStart="false" />
    </RadioGroup>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cable Size:"
        android:id="@+id/textView7"
        android:layout_above="@+id/radioGroup2"
        android:layout_centerHorizontal="true" />

    <RadioGroup
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/radioGroup4"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@+id/textView7"
        android:orientation="vertical"
        android:layout_toStartOf="@+id/button4">

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="8"
            android:id="@+id/radioButton10"
            android:checked="true"
            android:layout_alignBaseline="@+id/textView9"
            android:layout_alignBottom="@+id/textView9"
            android:layout_alignStart="@+id/textView7" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="7"
            android:id="@+id/radioButton11"
            android:layout_below="@+id/radioGroup4"
            android:layout_alignStart="@+id/radioGroup4"
            android:checked="false" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="6"
            android:id="@+id/radioButton12"
            android:layout_below="@+id/radioGroup4"
            android:layout_alignParentEnd="true"
            android:checked="false" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5"
            android:id="@+id/radioButton13"
            android:checked="false"
            android:layout_below="@+id/radioGroup4"
            android:layout_alignStart="@+id/radioGroup4" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="4"
            android:id="@+id/radioButton14"
            android:checked="false"
            android:layout_below="@+id/radioGroup4"
            android:layout_alignStart="@+id/radioGroup4" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:id="@+id/radioButton15"
            android:checked="false"
            android:layout_below="@+id/radioGroup4"
            android:layout_alignStart="@+id/radioGroup4" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2"
            android:id="@+id/radioButton16"
            android:layout_below="@+id/radioGroup4"
            android:layout_alignParentEnd="true"
            android:checked="false" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1"
            android:id="@+id/radioButton17"
            android:layout_alignTop="@+id/radioGroup4"
            android:layout_toEndOf="@+id/radioGroup4"
            android:checked="false" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1/0"
            android:id="@+id/radioButton18"
            android:layout_below="@+id/radioGroup4"
            android:layout_alignStart="@+id/radioGroup4"
            android:checked="false" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2/0"
            android:id="@+id/radioButton19"
            android:layout_below="@+id/radioGroup4"
            android:layout_alignParentEnd="true"
            android:checked="false" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3/0"
            android:id="@+id/radioButton20"
            android:layout_below="@+id/radioGroup4"
            android:layout_alignParentEnd="true"
            android:checked="false" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="4/0"
            android:id="@+id/radioButton21"
            android:layout_below="@+id/radioGroup4"
            android:layout_alignParentEnd="true"
            android:checked="false" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="250"
            android:id="@+id/radioButton22"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:checked="false" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="500"
            android:id="@+id/radioButton23"
            android:layout_alignParentBottom="true"
            android:layout_toStartOf="@+id/radioGroup4"
            android:checked="false" />

    </RadioGroup><![CDATA[


    android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView" />


]]>
</RelativeLayout>
